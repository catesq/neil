<?xml version="1.0" encoding="utf-8"?>
<zzub>
	<plugin
		uri="@trac.zeitherrschaft.org/aldrin/lunar/controller/transpose;1"
		name="Lunar Transpose"
		shortname="Transpose"
		type="controller"
		description="A simple controller which transposes note values."
		author="Leonard Ritter"
		email="contact at leonard-ritter+com">
		<parameters>
			<valuenamegroup>
				<valuenames id="octavetrans">
					<alias value="0" name="-5"/>
					<alias value="1" name="-4"/>
					<alias value="2" name="-3"/>
					<alias value="3" name="-2"/>
					<alias value="4" name="-1"/>
					<alias value="5" name="None"/>
					<alias value="6" name="+1"/>
					<alias value="7" name="+2"/>
					<alias value="8" name="+3"/>
					<alias value="9" name="+4"/>
					<alias value="10" name="+5"/>
				</valuenames>
				<valuenames id="notetrans">
					<alias value="0" name="-6"/>
					<alias value="1" name="-5"/>
					<alias value="2" name="-4"/>
					<alias value="3" name="-3"/>
					<alias value="4" name="-2"/>
					<alias value="5" name="-1"/>
					<alias value="6" name="None"/>
					<alias value="7" name="+1"/>
					<alias value="8" name="+2"/>
					<alias value="9" name="+3"/>
					<alias value="10" name="+4"/>
					<alias value="11" name="+5"/>
					<alias value="12" name="+6"/>
				</valuenames>
				<valuenames id="onoff">
					<alias value="0" name="Off"/>
					<alias value="1" name="On"/>
				</valuenames>
				<valuenames id="hquantizevals">
					<alias value="0" name="None"/>
					<alias value="1" name="Major"/>
					<alias value="2" name="Minor"/>
				</valuenames>
			</valuenamegroup>
			<global>
				<parameter
					id="otrans"
					name="Global Octave"
					description="Transpose note value globally (octave-wise)"
					type="byte"
					minvalue="0"
					maxvalue="10"
					defvalue="5"
					valuenames="octavetrans"
					state="true"/>
				<parameter
					id="ntrans"
					name="Global Note"
					description="Transpose note value globally (note-wise)"
					type="byte"
					minvalue="0"
					maxvalue="12"
					defvalue="6"
					valuenames="notetrans"
					state="true"/>
				<parameter
					id="hquantize"
					name="Harmonic Quantize"
					description="Quantize note harmonically"
					type="byte"
					minvalue="0"
					maxvalue="2"
					defvalue="0"
					valuenames="hquantizevals"
					state="true"/>
			</global>
			<track>
			</track>
			<controller>
				<parameter
					id="note1"
					name="Note #1"
					description="Transposes the note connected"
					type="midinote"/>
				<parameter
					id="note2"
					name="Note #2"
					description="Transposes the note connected"
					type="midinote"/>
				<parameter
					id="note3"
					name="Note #3"
					description="Transposes the note connected"
					type="midinote"/>
				<parameter
					id="note4"
					name="Note #4"
					description="Transposes the note connected"
					type="midinote"/>
				<parameter
					id="note5"
					name="Note #5"
					description="Transposes the note connected"
					type="midinote"/>
				<parameter
					id="note6"
					name="Note #6"
					description="Transposes the note connected"
					type="midinote"/>
				<parameter
					id="note7"
					name="Note #7"
					description="Transposes the note connected"
					type="midinote"/>
				<parameter
					id="note8"
					name="Note #8"
					description="Transposes the note connected"
					type="midinote"/>
				<parameter
					id="lnote1"
					name="Locked Note #1"
					description="Transposes the note connected, but keeps octave."
					type="midinote"/>
				<parameter
					id="lnote2"
					name="Locked Note #2"
					description="Transposes the note connected, but keeps octave."
					type="midinote"/>
				<parameter
					id="lnote3"
					name="Locked Note #3"
					description="Transposes the note connected, but keeps octave."
					type="midinote"/>
				<parameter
					id="lnote4"
					name="Locked Note #4"
					description="Transposes the note connected, but keeps octave."
					type="midinote"/>
				<parameter
					id="lnote5"
					name="Locked Note #5"
					description="Transposes the note connected, but keeps octave."
					type="midinote"/>
				<parameter
					id="lnote6"
					name="Locked Note #6"
					description="Transposes the note connected, but keeps octave."
					type="midinote"/>
				<parameter
					id="lnote7"
					name="Locked Note #7"
					description="Transposes the note connected, but keeps octave."
					type="midinote"/>
				<parameter
					id="lnote8"
					name="Locked Note #8"
					description="Transposes the note connected, but keeps octave."
					type="midinote"/>
			</controller>
		</parameters>
		<attributes>
		</attributes>
		<files>
			<file ref="transpose.cpp"/>
			<file ref="transpose.h"/>
		</files>
		<modules>
			<module id="transpose">
				<source language="c++" ref="transpose.cpp" fxdef="transpose.h"/>
			</module>
		</modules>
	</plugin>
</zzub>
