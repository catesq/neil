<?xml version="1.0" encoding="utf-8"?>
<zzub>
	<plugin
		uri="paniq/Generators/Kick"
		name="Lunar Kick"
		shortname="Kick"
		type="generator"
		mintracks="1"
		maxtracks="1"
		description="A kickdrum synth."
		author="Leonard Ritter"
		email="contact at leonard-ritter+com">
		<parameters>
			<global>
				<parameter
					id="attack"
					name="Attack (ms)"
					description="How long until a tone is fully amplified."
					type="float"
					logarithmic="true"
					minvalue="0.01"
					maxvalue="100"
					defvalue="0.01"
					precision="0.01"
					state="true"/>
				<parameter
					id="decay"
					name="Decay (ms)"
					description="How long until a tone is at sustain volume."
					type="float"
					logarithmic="true"
					minvalue="0.1"
					maxvalue="1000"
					defvalue="400"
					precision="0.1"
					state="true"/>
				<parameter
					id="sustain"
					name="Sustain (%)"
					description="Level of the sustain volume."
					type="float"
					logarithmic="true"
					minvalue="0"
					maxvalue="100"
					defvalue="0"
					precision="0.1"
					state="true"/>
				<parameter
					id="release"
					name="Release (ms)"
					description="How long until the tone is fully silent."
					type="float"
					logarithmic="true"
					minvalue="0.1"
					maxvalue="1000"
					defvalue="1"
					precision="0.1"
					state="true"/>
				<parameter
					id="startfreq"
					name="Start Tone"
					description="Starting frequency of tone."
					type="float"
					minvalue="110"
					maxvalue="11025"
					logarithmic="true"
					defvalue="2000"
					precision="1"
					state="true"/>
				<parameter
					id="endfreq"
					name="End Tone"
					description="Ending frequency of tone."
					type="float"
					minvalue="30"
					maxvalue="110"
					logarithmic="true"
					defvalue="50"
					precision="0.1"
					state="true"/>
				<parameter
					id="vdecay"
					name="Descent (ms)"
					description="Time of descent."
					type="float"
					logarithmic="true"
					minvalue="0.1"
					maxvalue="1000"
					defvalue="116"
					precision="0.1"
					state="true"/>
				<parameter
					id="power"
					name="Power"
					description="Bends the descent time."
					type="float"
					logarithmic="true"
					minvalue="0.1"
					maxvalue="8"
					defvalue="0.4"
					precision="0.01"
					state="true"/>
				<parameter
					id="amp"
					name="Master Gain (dB)"
					description="Final gain of output signal"
					type="float"
					minvalue="-48"
					maxvalue="0"
					defvalue="-6"
					precision="0.01"
					state="true"/>
			</global>
			<track>
				<parameter
					id="trigger"
					name="Kick"
					description="Trigger to kick."
					type="switch"
					defvalue="0"/>
				<parameter
					id="volume"
					name="Volume"
					description="Volume of kick."
					type="byte"
					minvalue="0"
					maxvalue="128"
					defvalue="128"
					novalue="255"/>
			</track>
		</parameters>
		<files>
			<file ref="kick.cpp"/>
			<file ref="kick.h"/>
			<file ref="adsr.h"/>
		</files>
		<modules>
			<module id="kick">
				<source language="c++" ref="kick.cpp" fxdef="kick.h"/>
			</module>
		</modules>
	</plugin>
</zzub>
