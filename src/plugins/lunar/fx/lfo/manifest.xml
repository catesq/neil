<?xml version="1.0" encoding="utf-8"?>
<zzub>
  <plugin
     uri="@trac.zeitherrschaft.org/aldrin/lunar/controller/LunarLFO;1"
     name="Lunar LFO"
     shortname="LFO"
     type="controller"
     description="A simple controller which emits an LFO wave."
     author="Leonard Ritter"
     email="contact at leonard-ritter+com">
    <parameters>
      <valuenamegroup>
	<valuenames id="wave">
	  <alias value="0" name="Sine"/>
	  <alias value="1" name="Saw"/>
	  <alias value="2" name="Square"/>
	  <alias value="3" name="Triangle"/>
	</valuenames>
      </valuenamegroup>
      <global>
	<parameter
	   id="wave"
	   name="Wave"
	   type="byte"
	   minvalue="0"
	   maxvalue="3"
	   defvalue="0"
	   valuenames="wave"
	   state="true"/>
	<parameter
	   id="rate"
	   name="Period"
	   type="word"
	   minvalue="2"
	   maxvalue="512"
	   defvalue="64"
	   state="true"/>
	<parameter
	   id="minimum"
	   name="Min"
	   type="float"
	   minvalue="0.0"
	   maxvalue="1.0"
	   defvalue="0.0"
	   precision="0.01"
	   state="true"/>
	<parameter
	   id="maximum"
	   name="Max"
	   type="float"
	   minvalue="0.0"
	   maxvalue="1.0"
	   defvalue="1.0"
	   precision="0.01"
	   state="true"/>
      </global>
      <track>
      </track>
      <controller>
	<parameter
	   id="out1"
	   name="LFO Out 1"
	   description="LFO Signal"
	   type="float"
	   minvalue="0"
	   maxvalue="1"
	   defvalue="0"
	   precision="0.01"/>
      </controller>
    </parameters>
    <attributes>
    </attributes>
    <files>
      <file ref="LunarLFO.cpp"/>
      <file ref="LunarLFO.h"/>
    </files>
    <modules>
      <module id="LunarLFO">
	<source language="c++" ref="LunarLFO.cpp" fxdef="LunarLFO.h"/>
      </module>
    </modules>
  </plugin>
</zzub>
