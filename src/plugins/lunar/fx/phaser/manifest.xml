<?xml version="1.0" encoding="utf-8"?>
<zzub>
	<plugin
		uri="@trac.zeitherrschaft.org/aldrin/lunar/effect/phaser;1"
		name="Lunar Phaser"
		shortname="Phaser"
		type="effect"
		description="A phasing effect"
		author="Pieter Holtzhausen"
		email="">
		<parameters>
			<global>
				<parameter
					id="drywet"
					name="Dry/Wet"
					description="Dry/Wet"
					type="float"
					minvalue="0"
					maxvalue="1"
					defvalue="0.5"
					precision="0.01"
					state="true"/>
				<parameter
					id="feedback"
					name="Feedback"
					description="Feedback"
					type="float"
					minvalue="0"
					maxvalue="1"
					defvalue="0.75"
					precision="0.01"
					state="true"/>	
				<parameter
					id="lfo_min"
					name="LFO Minimum (Hz)"
					description="LFO Minimum"
					type="float"
					minvalue="10"
					maxvalue="20000"
					defvalue="800"
					precision="1"
					state="true"/>	
				<parameter
					id="lfo_max"
					name="LFO Maximum (Hz)"
					description="LFO Maximum"
					type="float"
					minvalue="10"
					maxvalue="20000"
					defvalue="3200"
					precision="1"
					state="true"/>	
				<parameter
					id="lfo_rate"
					name="LFO rate (Hz)"
					description="Cutoff frequency of filter"
					type="float"
					logarithmic="true"
					power="0.7"
					minvalue="0.01"
					maxvalue="20000"
					defvalue="0.05"
					precision="0.01"
					state="true"/>
			</global>
			<track>
			</track>
		</parameters>
		<files>
			<file ref="phaser.cpp"/>
			<file ref="phaser.h"/>
		</files>
		<modules>
			<module id="phaser">
				<source language="c++" ref="phaser.cpp" fxdef="phaser.h"/>
			</module>
		</modules>
	</plugin>
</zzub>
