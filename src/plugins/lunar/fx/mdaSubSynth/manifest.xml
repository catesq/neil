<?xml version="1.0" encoding="utf-8"?>
<zzub>
  <plugin
     uri="SoMono/Effects/mdaSubSynth"
     name="Lunar mdaSubSynth"
     shortname="mdaSubSynth"
     type="effect"
     description="A low frequency amplifier/synthesizer by mda."
     author="Vytautas Jancauskas"
     email="unaudio@gmail.com">
    <parameters>
      <valuenamegroup>
	<valuenames id="synth_type">
	  <alias value="0" name="Distort"/>
	  <alias value="1" name="Divide"/>
	  <alias value="2" name="Invert"/>
	  <alias value="3" name="Key Osc."/>
	</valuenames>
      </valuenamegroup>
      <global>
	<parameter
	   id="fParam1"
	   name="Type"
	   description="Synthesizer type."
	   type="byte"
	   minvalue="0"
	   maxvalue="3"
	   defvalue="0"
	   valuenames="synth_type"
	   state="true"/>
	<parameter
	   id="fParam2"
	   name="Level (%)"
	   description="Level."
	   type="float"
	   minvalue="0"
	   maxvalue="100"
	   defvalue="30"
	   precision="0.01"
	   state="true"/>
	<parameter
	   id="fParam3"
	   name="Tune (Hz)"
	   description="Tune."
	   type="float"
	   minvalue="0"
	   maxvalue="1"
	   defvalue="0.35"
	   precision="0.001"
	   state="true"/>
	<parameter
	   id="fParam4"
	   name="Dry Mix (%)"
	   description="Dry Mix."
	   type="float"
	   minvalue="0"
	   maxvalue="100"
	   defvalue="100"
	   precision="0.01"
	   state="true"/>
	<parameter
	   id="fParam5"
	   name="Thresh (dB)"
	   description="Thresh."
	   type="float"
	   minvalue="-60"
	   maxvalue="0"
	   defvalue="-20"
	   precision="0.01"
	   state="true"/>
	<parameter
	   id="fParam6"
	   name="Release (ms)"
	   description="Release."
	   type="float"
	   minvalue="0"
	   maxvalue="1"
	   defvalue="0.35"
	   precision="0.01"
	   state="true"/>
      </global>
    </parameters>
    <files>
      <file ref="mdaSubSynth.cpp"/>
      <file ref="mdaSubSynth.h"/>
    </files>
    <modules>
      <module id="mdaSubSynth">
	<source language="c++" ref="mdaSubSynth.cpp" fxdef="mdaSubSynth.h"/>
      </module>
    </modules>
  </plugin>
</zzub>
