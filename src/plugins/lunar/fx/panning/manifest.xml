<?xml version="1.0" encoding="utf-8"?>
<zzub>
	<plugin
		uri="@trac.zeitherrschaft.org/aldrin/lunar/effect/panning;1"
		name="Lunar Pan"
		shortname="Pan"
		type="effect"
		description="A panning effect."
		author="Leonard Ritter"
		email="contact at leonard-ritter+com">
		<parameters>
			<global>
				<parameter
					id="pan"
					name="Angle"
					description="Panning of Signal"
					type="float"
					minvalue="0"
					maxvalue="1"
					defvalue="0.5"
					precision="0.01"
					state="true"/>
			</global>
			<track>
			</track>
		</parameters>
		<files>
			<file ref="panning.cpp"/>
			<file ref="panning.h"/>
		</files>
		<modules>
			<module id="panning">
				<source language="c++" ref="panning.cpp" fxdef="panning.h"/>
			</module>
		</modules>
	</plugin>
</zzub>
