<?xml version="1.0" encoding="utf-8"?>
<zzub>
  <plugin
     uri="@trac.zeitherrschaft.org/aldrin/lunar/generator/superfm;1"
     name="Zephod SuperFM"
     shortname="SuperFM"
     type="generator"
     mintracks="1"
     maxtracks="8"
     description="A port of Zephod SuperFM synthesizer to Lunar."
     author="Vytautas Jancauskas"
     email="unaudio@gmail.com">
    <parameters>
      <valuenamegroup>
	<valuenames id="waveforms">
	  <alias value="0" name="Sine"/>
	  <alias value="1" name="Square"/>
	  <alias value="2" name="+Saw"/>
	  <alias value="3" name="-Saw"/>
	</valuenames>
      </valuenamegroup>
      <global>
	<parameter
	   id="paraRoute"
	   name="FM Rout Mode"
	   description="Modulator Routing Mode"
	   type="word"
	   minvalue="0"
	   maxvalue="3"
	   defvalue="0"
	   precision="1"
	   state="true"/>
	<parameter
	   id="paraAttack"
	   name="VCA Env A (ms)"
	   description="VCA Attack in samples"
	   type="float"
	   logarithmic="true"
	   power="0.7"
	   minvalue="1.0"
	   maxvalue="5000.0"
	   defvalue="5.0"
	   precision="1.0"
	   state="true"/>
	<parameter
	   id="paraDecay"
	   name="VCA Env D (ms)"
	   description="VCA Decay in samples"
	   type="float"
	   logarithmic="true"
	   power="0.7"
	   minvalue="1.0"
	   maxvalue="5000.0"
	   defvalue="500.0"
	   precision="1.0"
	   state="true"/>
	<parameter
	   id="paraSustainv"
	   name="VCA Env S (%)"
	   description="VCA Sustain level"
	   type="byte"
	   minvalue="0"
	   maxvalue="100"
	   defvalue="20"
	   state="true"/>
	<parameter
	   id="paraRelease"
	   name="VCA Env R (ms)"
	   description="VCA Release in samples"
	   type="float"
	   logarithmic="true"
	   power="0.7"
	   minvalue="1.0"
	   maxvalue="5000.0"
	   defvalue="50.0"
	   precision="1.0"
	   state="true"/>
	<parameter
	   id="paraMAttack"
	   name="MOD Env A (ms)"
	   description="MOD Envelope attack in samples"
	   type="float"
	   logarithmic="true"
	   power="0.7"
	   minvalue="1.0"
	   maxvalue="5000.0"
	   defvalue="5.0"
	   precision="1.0"
	   state="true"/>
	<parameter
	   id="paraMDecay"
	   name="MOD Env D (ms)"
	   description="MOD Envelope decay in samples"
	   type="float"
	   logarithmic="true"
	   power="0.7"
	   minvalue="1.0"
	   maxvalue="5000.0"
	   defvalue="500.0"
	   precision="1.0"
	   state="true"/>
	<parameter
	   id="paraMSustainv"
	   name="MOD Env S (%)"
	   description="MOD Envelope sustain level"
	   type="byte"
	   minvalue="0"
	   maxvalue="100"
	   defvalue="20"
	   state="true"/>
	<parameter
	   id="paraMRelease"
	   name="MOD Env R (ms)"
	   description="MOD Envelope release in samples"
	   type="float"
	   logarithmic="true"
	   power="0.7"
	   minvalue="1.0"
	   maxvalue="5000.0"
	   defvalue="50.0"
	   precision="1.0"
	   state="true"/>
	<parameter
	   id="paraModNote1D"
	   name="MOD1 Depth"
	   description="Modulator 1 depth"
	   type="float"
	   minvalue="0.0"
	   maxvalue="1.0"
	   defvalue="0.2"
	   precision="0.001"
	   state="true"/>
	<parameter
	   id="paraModNote2D"
	   name="MOD2 Depth"
	   description="Modulator 2 depth"
	   type="float"
	   minvalue="0.0"
	   maxvalue="1.0"
	   defvalue="0.0"
	   precision="0.001"
	   state="true"/>
	<parameter
	   id="paraModNote3D"
	   name="MOD3 Depth"
	   description="Modulator 3 depth"
	   type="float"
	   minvalue="0.0"
	   maxvalue="1.0"
	   defvalue="0.0"
	   precision="0.001"
	   state="true"/>
	<parameter
	   id="paraModEnv1"
	   name="MOD1 Env"
	   description="Modulator 1 envelope amount"
	   type="float"
	   minvalue="-1.0"
	   maxvalue="1.0"
	   defvalue="0.5"
	   precision="0.001"
	   state="true"/>
	<parameter
	   id="paraModEnv2"
	   name="MOD2 Env"
	   description="Modulator 2 envelope amount"
	   type="float"
	   minvalue="-1.0"
	   maxvalue="1.0"
	   defvalue="0.2"
	   precision="0.001"
	   state="true"/>
	<parameter
	   id="paraModEnv3"
	   name="MOD3 Env"
	   description="Modulator 3 envelope amount"
	   type="float"
	   minvalue="-1.0"
	   maxvalue="1.0"
	   defvalue="0.3"
	   precision="0.001"
	   state="true"/>
	<parameter
	   id="lfoFreq"
	   name="LFO Freq (Hz)"
	   description="LFO Frequency"
	   type="float"
	   logarithmic="true"
	   power="0.7"
	   minvalue="0.001"
	   maxvalue="32.0"
	   defvalue="0.3"
	   precision="0.001"
	   state="true"/>
	<parameter
	   id="lfoMod1"
	   name="MOD1 Lfo"
	   description="Amount of LFO modulation to MOD1"
	   type="float"
	   minvalue="-1.0"
	   maxvalue="1.0"
	   defvalue="0.0"
	   precision="0.001"
	   state="true"/>
	<parameter
	   id="lfoMod2"
	   name="MOD2 Lfo"
	   description="Amount of LFO modulation to MOD2"
	   type="float"
	   minvalue="-1.0"
	   maxvalue="1.0"
	   defvalue="0.0"
	   precision="0.001"
	   state="true"/>
	<parameter
	   id="lfoMod3"
	   name="MOD3 Lfo"
	   description="Amount of LFO modulation to MOD3"
	   type="float"
	   minvalue="-1.0"
	   maxvalue="1.0"
	   defvalue="0.0"
	   precision="0.001"
	   state="true"/>
	</global>
      <track>
	<parameter
	   id="note"
	   type="note"/>
	<parameter
	   id="volume"
	   name="Volume"
	   description="Volume of note."
	   type="byte"
	   minvalue="0"
	   maxvalue="128"
	   defvalue="128"
	   novalue="255"/>
      </track>
    </parameters>
      <files>
      <file ref="envelope.cpp"/>
      <file ref="envelope.hpp"/>
      <file ref="zephod_superfm.cpp"/>
      <file ref="zephod_superfm.h"/>
    </files>
    <modules>
      <module id="superfm">
	<source language="c++" ref="zephod_superfm.cpp" fxdef="zephod_superfm.h"/>
      </module>
    </modules>
  </plugin>
</zzub>
