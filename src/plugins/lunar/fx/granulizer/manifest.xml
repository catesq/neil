<?xml version="1.0" encoding="utf-8"?>
<zzub>
  <plugin
     uri="@trac.zeitherrschaft.org/aldrin/lunar/effects/LunarGranulizer;1"
     name="Lunar Granulizer"
     shortname="Granulizer"
     type="effect"
     description="A granulizer effect."
     author="Vytautas Jancauskas"
     email="unaudio@gmail.com">
    <parameters>
      <global>
 	<parameter
	   id="dry_wet"
	   name="Dry/Wet"
	   description="Dry/Wet balance."
	   type="float"
	   minvalue="0"
	   maxvalue="1"
	   defvalue="1"
	   precision="0.01"
	   state="true"/>
	<parameter
	   id="number_of_grains"
	   name="Grains"
	   description="Number of grains active."
	   type="word"
	   minvalue="0"
	   maxvalue="127"
	   defvalue="32"
	   state="true"/>
	<parameter
	   id="grain_length"
	   name="Length"
	   description="Length of a grain."
	   type="word"
	   minvalue="0"
	   maxvalue="8192"
	   defvalue="256"
	   state="true"/>
 	<parameter
	   id="attack"
	   name="Attack"
	   description="Grain envelope attack."
	   type="float"
	   minvalue="0"
	   maxvalue="1"
	   defvalue="0.2"
	   precision="0.01"
	   state="true"/>
 	<parameter
	   id="sustain"
	   name="Sustain"
	   description="Grain envelope sustain."
	   type="float"
	   minvalue="0"
	   maxvalue="1"
	   defvalue="0.6"
	   precision="0.01"
	   state="true"/>
 	<parameter
	   id="min_amp"
	   name="Min Amp"
	   description="Minimum grain amplitude."
	   type="float"
	   minvalue="0"
	   maxvalue="1"
	   defvalue="0.2"
	   precision="0.01"
	   state="true"/>
 	<parameter
	   id="max_amp"
	   name="Max Amp"
	   description="Maximum grain amplitude."
	   type="float"
	   minvalue="0"
	   maxvalue="1"
	   defvalue="0.5"
	   precision="0.01"
	   state="true"/>
 	<parameter
	   id="pstart"
	   name="PStart"
	   description="Start playback speed."
	   type="float"
	   minvalue="0"
	   maxvalue="8"
	   defvalue="1.0"
	   precision="0.01"
	   state="true"/>
 	<parameter
	   id="pend"
	   name="PEnd"
	   description="End playback speed."
	   type="float"
	   minvalue="0"
	   maxvalue="8"
	   defvalue="1.0"
	   precision="0.01"
	   state="true"/>
	<parameter
	   id="min_delay"
	   name="Min Delay"
	   description="Minimum delay time for a grain."
	   type="float"
	   minvalue="0"
	   maxvalue="4"
	   defvalue="0.0"
	   precision="0.01"
	   state="true"/>
	<parameter
	   id="max_delay"
	   name="Max Delay"
	   description="Maximum delay time for a grain."
	   type="float"
	   minvalue="0"
	   maxvalue="4"
	   defvalue="2.0"
	   precision="0.01"
	   state="true"/>
 	<parameter
	   id="feedback"
	   name="Feedback"
	   description="Effect feedback."
	   type="float"
	   minvalue="0"
	   maxvalue="1"
	   defvalue="0.5"
	   precision="0.01"
	   state="true"/>
	</global>
    </parameters>
      <files>
      <file ref="LunarGranulizer.cpp"/>
      <file ref="LunarGranulizer.hpp"/>
    </files>
    <modules>
      <module id="LunarGranulizer">
	<source language="c++" ref="LunarGranulizer.cpp" fxdef="LunarGranulizer.hpp"/>
      </module>
    </modules>
  </plugin>
</zzub>
